<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
">
    <bean id="captain" class="Player">
        <property name="name" value="Harry Kame" />
        <property name="number" value="10" />
    </bean>
    <bean id="goal-keeper" class="Player">
        <property name="name" value="Jordan Pickford" />
        <property name="number" value="1" />
    </bean>
    <bean id="player-5" class="Player">
        <property name="name" value="Harry Maguire" />
        <property name="number" value="5" />
    </bean>

</beans>
